<addons default-repository="github-ssh">

    <!-- Moqui runtime -->
    <runtime name="moqui-runtime" group="daoflows" version="" branch="master"/>

    <!-- Moqui components -->
    <component name="moqui-aws" group="daoflows" version="" branch="master"/>
    <component name="moqui-fop" group="daoflows" version="" branch="master"/>
    <component name="moqui-poi" group="daoflows" version="" branch="master"/>
    <component name="moqui-sftp" group="daoflows" version="" branch="master"/>
    <component name="mantle-udm" group="daoflows" version="" branch="master"/>
    <component name="mantle-usl" group="daoflows" version="" branch="master"/>
    <component name="SimpleScreens" group="daoflows" version="" branch="master"/>
    <component name="MarbleERP" group="daoflows" version="" branch="master"/>

    <!-- DAOLOS components -->
    <component name="daolos-services" group="daoflows" version="" branch="master"/>
    <component name="daolos-store" group="daoflows" version="" branch="master"/>
    <component name="daolos-backoffice" group="daoflows" version="" branch="master"/>

    <!-- Component sets -->
